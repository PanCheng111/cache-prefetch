{
  "name": "Profile_Based_Cache_Prefetching",
  "version": "1.0.0",
  "description": "A cross platform desktop app that lets you save your frequently used code snippets.",
  "main": "./main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist-mac": "build --mac",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron . --dev"
  },
  "build": {
    "appId": "coding-blocks.Snipper",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fs": "^0.0.1-security",
    "nedb": "^1.8.0",
    "node-c-parser": "^1.0.11",
    "path": "^0.12.7",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "^18.3.5"
  }
}
